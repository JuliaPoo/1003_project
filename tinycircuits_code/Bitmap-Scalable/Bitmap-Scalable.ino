const unsigned char Rx = 96;
const unsigned char Ry = 64;
unsigned char LINEBUFFER[Rx];

void setup() {
  Wire.begin();
  display.begin();
  display.setBrightness(15);
}

void loop() {

  unsigned char frame;
  for (frame = 0; frame < 8*7; frame++){
    DrawScreen(frame);
    delay(20);
  }
  
}

void DrawScreen(unsigned char frame){
  // Arrange sprite in increasing z-order

  // Initialise frame numbers
  unsigned char CUTIE_FRAME, FOXY_FRAME, DOGGY_FRAME;
  CUTIE_FRAME = frame%8;
  FOXY_FRAME = frame%7;
  DOGGY_FRAME = frame%8;
  
  // Initialise frame sizes
  unsigned char CUTIE_X, CUTIE_Y, FOXY_X, FOXY_Y, CLOUD1_X, CLOUD1_Y, CLOUD2_X, CLOUD2_Y, GRASS_X, GRASS_Y, DOGGY_X, DOGGY_Y;
  CUTIE_X = pgm_read_byte_near(CUTIE_XY + (CUTIE_FRAME)*2);
  CUTIE_Y = pgm_read_byte_near(CUTIE_XY + (CUTIE_FRAME)*2 + 1);
  FOXY_X = pgm_read_byte_near(FOXY_XY + (FOXY_FRAME)*2);
  FOXY_Y = pgm_read_byte_near(FOXY_XY + (FOXY_FRAME)*2 + 1);
  DOGGY_X = pgm_read_byte_near(DOGGY_XY + (DOGGY_FRAME)*2);
  DOGGY_Y = pgm_read_byte_near(DOGGY_XY + (DOGGY_FRAME)*2 + 1);
  CLOUD1_X = pgm_read_byte_near(CLOUD1_XY);
  CLOUD1_Y = pgm_read_byte_near(CLOUD1_XY + 1);
  CLOUD2_X = pgm_read_byte_near(CLOUD2_XY);
  CLOUD2_Y = pgm_read_byte_near(CLOUD2_XY + 1);
  GRASS_X = pgm_read_byte_near(GRASS_XY);
  GRASS_Y = pgm_read_byte_near(GRASS_XY + 1);

  // Sets cursor
  display.setX(0, Rx-1);
  display.setY(0, Ry-1);
  display.startData();
  
  unsigned char LINE_N, var;
  for (LINE_N = 0; LINE_N < Ry; LINE_N++){
    
    //Draw background
    for (var = 0; var < Rx; var++) LINEBUFFER[var] = BACKGROUND;

    //Draw Ground
    if (LINE_N > Ry - 10) for (var = 0; var < Rx; var++) LINEBUFFER[var] = GROUND;
    DrawSprite(GRASS, GRASS_X, GRASS_Y, (unsigned char)(Rx*((float)(57-frame)/57)), Ry-13, LINE_N);

    //Draw Clouds
    DrawSprite(CLOUD1, CLOUD1_X, CLOUD1_Y, (unsigned char)(2*Rx*((float)(57-frame)/57)), 10, LINE_N);
    DrawSprite(CLOUD2, CLOUD2_X, CLOUD2_Y, (unsigned char)(Rx*((float)(57-frame)/57) + 50), 15, LINE_N);

    //Draw CUTIE
    DrawSprite(CUTIE_ARR[CUTIE_FRAME], CUTIE_X, CUTIE_Y, Rx/2 - 3*CUTIE_X/2, Ry/2 - CUTIE_Y/2 + 10, LINE_N);

    //Draw FOXY
    DrawSprite(FOXY_ARR[FOXY_FRAME], FOXY_X, FOXY_Y, Rx/2 - 3*FOXY_X/2 + 5, Ry/2 - FOXY_Y/2 + 10 + 10, LINE_N);

    //Draw DOGGY
    DrawSprite(DOGGY_ARR[DOGGY_FRAME], DOGGY_X, DOGGY_Y, Rx/2 - 3*DOGGY_X/2 + 45, Ry/2 - DOGGY_Y/2 + 10 + 8, LINE_N);

    display.writeBuffer(LINEBUFFER, Rx);
  }
  display.endTransfer();
}

void DrawSprite(long SpritePointer, unsigned char SpriteSize_X,  unsigned char SpriteSize_Y, unsigned char x, unsigned char y, unsigned char line_n){
  // SpritePointer is the pointer to the bitmap data (in progmem)
  // SpriteSize_X is the x-size of the bitmap
  // SpriteSize_Y is the y-size of the bitmap
  // x is the position to plot (left side coord)
  // y is the position to plot (bottom side coord)

  if (line_n < y || line_n >= y + SpriteSize_Y){return;}
  
  unsigned char pos, pixel;
  for (pos = x; pos < x + SpriteSize_X; pos++){
    pixel = pgm_read_byte_near(SpritePointer + SpriteSize_X*(line_n - y) + pos - x);
    if (pixel != ALPHA) LINEBUFFER[pos%Rx] = pixel;
  }
}
